@model IEnumerable<Flashcards.Models.Question>

@{
    ViewBag.Title = "Stats";
}

<h2>Stats</h2>


<table class="table">
    <tr>
        <td>Word1</td>
        <td>Word2</td>
        <td>Score</td>
    </tr>
    @foreach (var item in Model)
    {
<tr>
  
        <td>
            @Html.DisplayFor(model => item.flashCard.Word1)
        </td>

    <td>
        @Html.DisplayFor(model => item.flashCard.Word2)
    </td>
    
    <td>
        @Html.DisplayFor(modelItem => item.Score)
    </td>


</tr>
    }




    </table>
